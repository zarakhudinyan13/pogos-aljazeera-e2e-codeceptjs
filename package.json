 "scripts": {
    "selenium:install": "npx selenium-standalone install",
    "selenium:start": "npx selenium-standalone start",
    "test:most-read": "codeceptjs run --features --grep @most-read --steps",
    "test:live": "codeceptjs run --features --grep @live --steps",
    "test:all": "codeceptjs run-multiple parallel --features --steps",
    "test:ci": "HEADLESS=true DONT_FAIL_ON_EMPTY_RUN=true npm run test:all",
    "report": "allure generate output --clean && allure open"
   }
